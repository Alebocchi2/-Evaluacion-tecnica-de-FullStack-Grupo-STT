{"ast":null,"code":"import EventCard from \"@/componentes/EventCard.vue\";\nimport Servicios from \"@/servicios/ServiciosApi.js\";\nexport default {\n  //props: [\"page\"],\n  name: \"Inicio\",\n  components: {\n    EventCard\n  },\n\n  data() {\n    return {\n      events: null,\n      pagina: 1\n    };\n  },\n\n  created() {\n    Servicios.getPersonajes().then(response => {\n      this.events = response.data.results;\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    siguiente: function () {\n      this.pagina++;\n      this.getPagina();\n    },\n    anterior: function () {\n      this.pagina < 2 ? this.pagina : this.pagina--;\n      this.getPagina();\n    },\n    paginas: function (pag) {\n      this.pagina = pag;\n      this.getPagina();\n    },\n    getPagina: function () {\n      Servicios.getPersonajesXpagina(this.pagina).then(response => {\n        this.events = response.data.results;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  computed: {\n    muestroPagina() {\n      if (this.pagina === 1) {\n        return 0;\n      } else return 1;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,SAAP,MAAsB,6BAAtB;AACA,OAAOC,SAAP,MAAsB,6BAAtB;AAEA,eAAe;AACd;AACAC,MAAI,EAAE,QAFQ;AAGdC,YAAU,EAAE;AACXH;AADW,GAHE;;AAMdI,MAAI,GAAG;AACN,WAAO;AACNC,YAAM,EAAE,IADF;AAENC,YAAM,EAAE;AAFF,KAAP;AAIA,GAXa;;AAYdC,SAAO,GAAG;AACTN,aAAS,CAACO,aAAV,GACEC,IADF,CACQC,QAAD,IAAc;AACnB,WAAKL,MAAL,GAAcK,QAAQ,CAACN,IAAT,CAAcO,OAA5B;AACA,KAHF,EAIEC,KAJF,CAISC,KAAD,IAAW;AACjBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACA,KANF;AAOA,GApBa;;AAqBdG,SAAO,EAAE;AACRC,aAAS,EAAE,YAAY;AACtB,WAAKX,MAAL;AACA,WAAKY,SAAL;AACA,KAJO;AAKRC,YAAQ,EAAE,YAAY;AACrB,WAAKb,MAAL,GAAc,CAAd,GAAkB,KAAKA,MAAvB,GAAgC,KAAKA,MAAL,EAAhC;AACA,WAAKY,SAAL;AACA,KARO;AASRE,WAAO,EAAE,UAAUC,GAAV,EAAe;AACvB,WAAKf,MAAL,GAAce,GAAd;AACA,WAAKH,SAAL;AACA,KAZO;AAaRA,aAAS,EAAE,YAAY;AACtBjB,eAAS,CAACqB,oBAAV,CAA+B,KAAKhB,MAApC,EACEG,IADF,CACQC,QAAD,IAAc;AACnB,aAAKL,MAAL,G