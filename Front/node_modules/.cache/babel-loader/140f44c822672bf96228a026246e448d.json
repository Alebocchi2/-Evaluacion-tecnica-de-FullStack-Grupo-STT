{"ast":null,"code":"import EventCard from \"@/components/EventCard.vue\";\nimport EventService from \"@/services/EventService.js\"; //import axios from 'axios'\n\nexport default {\n  //props: [\"page\"],\n  name: \"CardList\",\n  components: {\n    EventCard\n  },\n\n  data() {\n    return {\n      events: null,\n      page: 1\n    };\n  },\n\n  created() {\n    EventService.getEvents().then(response => {\n      console.log(\"events:\", response.data.results);\n      this.events = response.data.results;\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    siguiente: function () {\n      this.page++;\n      this.getPage();\n    },\n    anterior: function () {\n      this.page < 2 ? this.page : this.page--;\n      this.getPage();\n    },\n    pages: function () {\n      //this.page < 2 ? this.page : this.page--;\n      this.getPage();\n    },\n    getPage: function () {\n      EventService.getEventsPage(this.page).then(response => {\n        this.events = response.data.results;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  computed: {\n    muestroPage() {\n      if (this.page === 1) {\n        return 0;\n      } else return 1;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,YAAP,MAAyB,4BAAzB,C,CACA;;AAEA,eAAe;AACd;AACAC,MAAI,EAAE,UAFQ;AAGdC,YAAU,EAAE;AACXH;AADW,GAHE;;AAMdI,MAAI,GAAG;AACN,WAAO;AACNC,YAAM,EAAE,IADF;AAENC,UAAI,EAAE;AAFA,KAAP;AAIA,GAXa;;AAYdC,SAAO,GAAG;AACTN,gBAAY,CAACO,SAAb,GACEC,IADF,CACQC,QAAD,IAAc;AACnBC,aAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBF,QAAQ,CAACN,IAAT,CAAcS,OAArC;AACA,WAAKR,MAAL,GAAcK,QAAQ,CAACN,IAAT,CAAcS,OAA5B;AACA,KAJF,EAKEC,KALF,CAKSC,KAAD,IAAW;AACjBJ,aAAO,CAACC,GAAR,CAAYG,KAAZ;AACA,KAPF;AAQA,GArBa;;AAsBdC,SAAO,EAAE;AACRC,aAAS,EAAE,YAAY;AACtB,WAAKX,IAAL;AACA,WAAKY,OAAL;AACA,KAJO;AAKRC,YAAQ,EAAE,YAAY;AACrB,WAAKb,IAAL,GAAY,CAAZ,GAAgB,KAAKA,IAArB,GAA4B,KAAKA,IAAL,EAA5B;AACA,WAAKY,OAAL;AACA,KARO;AASRE,SAAK,EAAE,YAAY;AAClB;AACA,WAAKF,OAAL